{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "scl",
      "camelcase": "Scl",
      "title": "Scl",
      "scope": "source.scl",
      "file-types": [
        "scl",
        "udt",
        "db"
      ],
      "injection-regex": "^scl$",
      "class-name": "TreeSitterScl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "SCL grammar for tree-sitter",
    "authors": [
      {
        "name": "Martin Heubuch",
        "email": "martin.heubuch@spie-escad.de"
      }
    ],
    "links": {
      "repository": "https://github.com/tastenmo/tree-sitter-scl.git"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}